{
  "entryPoints": ["src/index.ts"],
  "tsconfig": "./tsconfig.json",
  "entryPointStrategy": "resolve",
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeNotDocumented": true,
  "excludeNotDocumentedKinds": [
    "Module",
    "Namespace"
  ]
}
